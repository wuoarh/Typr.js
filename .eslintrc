{
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
	},
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "globals": {
        "lang": false
    },
    "rules": {
        "indent": [
            "error", "tab",
            {"SwitchCase": 1}
        ],
        "no-console": "warn",
        "no-useless-escape": "warn",
        "no-prototype-builtins": "warn"
    }
}
